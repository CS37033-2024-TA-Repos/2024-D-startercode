{
  "name": "prettier-config-custom",
  "private": true,
  "version": "1.0.0",
  "dependencies": {
    "prettier": "latest"
  },
  "scripts": {
    "shared:prettierlint": "cd $INIT_CWD && prettier --ignore-path ${PROJECT_CWD}/configs/prettier-config-custom/.prettierignore --check .",
    "shared:prettierlint:fix": "cd $INIT_CWD && prettier --ignore-path ${PROJECT_CWD}/configs/prettier-config-custom/.prettierignore --write ."
  }
}
